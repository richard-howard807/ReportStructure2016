      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT  mainquery.*, ISNULL(workdaysall.number_of_work_days,0) number_of_work_days  
FROM 
(SELECT 


--fin_days.gl_fin_day_in_month [glfinday]
       CASE WHEN fin_days.gl_cal_day_in_week = 'Sat' THEN   [fin_days].gl_calendar_date -1 
          WHEN  fin_days.gl_cal_day_in_week = 'Sun' THEN   [fin_days].gl_calendar_date -2
          WHEN fin_days.gl_cal_day_in_week = 'Mon' AND fin_days.gl_holiday_flag = 'Y' THEN [fin_days].gl_calendar_date -2 
          ELSE fin_days.gl_calendar_date 
          END 
          
           [Calender]
              , fin_days.gl_fin_day_in_month
              , fin_days.gl_fin_period
          
       , fin_days.gl_trading_days_in_mth [trading]
       , fin_days.gl_week_end_flag
       , fin_days.gl_holiday_flag
    --   , [fin_days].gl_calendar_date
       , fin_days.gl_cal_month_name
   --    , [fin_days].gl_cal_day_in_week 
       , hierarchylevel2hist
       , hierarchylevel3hist 
       , ISNULL(SUM([Hours].[Hours Recorded]), 0) as [Hours Recorded]
    --      ,ISNULL(workdays.number_of_work_days,0) number_of_work_days


FROM red_dw.dbo.dim_gl_date as [fin_month]
--INNER JOIN red_dw.dbo.dim_gl_date as [fin_days] on [fin_days].gl_fin_month=[fin_month].gl_fin_month 
INNER JOIN red_dw.dbo.dim_gl_date as [fin_days] on [fin_days].dim_gl_date_key=[fin_month].dim_gl_date_key
and [fin_days].gl_calendar_date&lt;=CAST(GETDATE()-1 as date) 
AND fin_days.gl_calendar_date &gt;= '2019-03-01'


LEFT OUTER JOIN  (
SELECT dim_date.fin_month_no,dim_date.fin_year,first_day_month.dim_date_key first_day_key,COUNT(*) number_of_work_days 
FROM red_dw.dbo.dim_date
JOIN red_Dw.dbo.[dim_date] first_day_month
ON first_day_month.fin_month = dim_date.fin_month
and first_day_month.fin_year = dim_date.fin_year
and first_day_month.cal_day_in_month = 1
WHERE dim_date.holiday_flag = 'N'
AND dim_date.fin_day_in_week NOT IN ('Sat','Sun')
AND dim_date.calendar_date &lt;= getdate() -1
GROUP BY dim_date.fin_month_no,dim_date.fin_year,first_day_month.dim_date_key )
AS  workdays ON workdays.first_day_key = fin_days.dim_gl_date_key



INNER JOIN (SELECT dim_orig_posting_date_key dim_gl_date_key, hierarchylevel2hist, hierarchylevel3hist,  ISNULL(SUM(minutes_recorded),0) / 60 as [Hours Recorded]
                     FROM red_dw.dbo.fact_billable_time_activity
                     INNER JOIN red_dw.dbo.dim_fed_hierarchy_history as [Current Hierarchy]
                     on [Current Hierarchy].dim_fed_hierarchy_history_key=fact_billable_time_activity.dim_fed_hierarchy_history_key
                     and hierarchylevel2hist in ('Legal Ops - Claims','Legal Ops - LTA')
                     GROUP BY dim_orig_posting_date_key, hierarchylevel2hist, hierarchylevel3hist
                     ) [Hours] on [Hours].dim_gl_date_key=[fin_days].dim_gl_date_key
       
where [fin_month].gl_calendar_date&lt;= CAST(GETDATE()-1 as date)
AND fin_days.gl_calendar_date &gt;= '2019-03-01'
--AND fin_days.gl_fin_period = @Period



GROUP BY      CASE
            WHEN fin_days.gl_cal_day_in_week = 'Sat' THEN
            [fin_days].gl_calendar_date - 1
            WHEN fin_days.gl_cal_day_in_week = 'Sun' THEN
            [fin_days].gl_calendar_date - 2
            WHEN fin_days.gl_cal_day_in_week = 'Mon'
            AND fin_days.gl_holiday_flag = 'Y' THEN
            [fin_days].gl_calendar_date - 2
            ELSE
            fin_days.gl_calendar_date
            END,
           -- fin_days.gl_fin_day_in_month,
            fin_days.gl_trading_days_in_mth,
            fin_days.gl_week_end_flag,
            fin_days.gl_holiday_flag,
            fin_days.gl_cal_month_name,
            Hours.hierarchylevel2hist,
            Hours.hierarchylevel3hist
                     , fin_days.gl_fin_day_in_month
                     ,workdays.number_of_work_days
                     ,fin_days.gl_fin_period
UNION

/*------------------------------------------------------------------------------------*/

SELECT DISTINCT  

       
           [Calender],

gl_fin_day_in_month,gl_fin_period,trading
,gl_week_end_flag,gl_holiday_flag,gl_cal_month_name
